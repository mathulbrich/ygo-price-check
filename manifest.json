{
  "manifest_version": 3,
  "name": "Yugioh Extension",
  "version": "1.0",
  "description": "Logs content of element with class 'nome-auxiliar'",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://www.ligayugioh.com.br/*"],
      "js": ["contentScript.js"]
    }
  ],
  "icons": {
    "48": "icon.png"
  }
}
